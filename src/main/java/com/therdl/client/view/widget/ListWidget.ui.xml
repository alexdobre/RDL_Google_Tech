<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

	<ui:style>
		.margin-fix {
		margin-bottom: 0;
		margin-top: 0;
		padding-bottom: 2px;
		padding-top: 0;
		float: right;
		}
	</ui:style>

	<g:HTMLPanel>
		<b:Row>
			<b:Column size="MD_6" offset="MD_6">
				<b:Pagination addStyleNames="{style.margin-fix}">
					<b:AnchorListItem ui:field="listRange" addStyleNames="disabled"/>
					<b:AnchorListItem ui:field="prevPage" icon="ANGLE_DOUBLE_LEFT"/>
					<b:AnchorListItem ui:field="nextPage" icon="ANGLE_DOUBLE_RIGHT"/>
				</b:Pagination>
			</b:Column>
		</b:Row>
		<b:Row>
			<b:Column size="MD_12">
				<b:LinkedGroup ui:field="listGroup"/>
			</b:Column>
		</b:Row>
	</g:HTMLPanel>
</ui:UiBinder>