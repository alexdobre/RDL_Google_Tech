             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>

    </ui:style>
    <g:HTMLPanel>
        <g:MenuBar ui:field="menuBar" >

            <g:MenuItem text="Home" ui:field = "home"/>
