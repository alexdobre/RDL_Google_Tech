                        <b:ListItem text="{i18n.email}" ui:field="email"></b:ListItem>
                        <b:ListItem text="{i18n.userProfile}" ui:field="profile"></b:ListItem>
                        <b:ListItem text="{i18n.logOut}" ui:field="out"></b:ListItem>
                    </b:DropDownMenu>
                </b:ListDropDown>
            </b:NavbarNav>
        </b:NavbarCollapse>
