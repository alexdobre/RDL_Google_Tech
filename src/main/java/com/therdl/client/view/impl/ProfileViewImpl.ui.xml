<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:rdl="urn:import:com.therdl.client.view.widget">

    <ui:style>

        .outer {
        position:relative;
        display:block;
        width: 100%;
        height:100%;
        margin: 0 auto;


        }

        .headerText {
         font-size:14px;
         margin:20px;
         top:40px;
         display:block;

        }




    </ui:style>
    <g:HTMLPanel addStyleNames="{style.outer}">
        <rdl:AppMenu ui:field="appMenu" />

        <span  class="{style.headerText}">User Profile Data</span>


        <g:FlowPanel ui:field = "profileImagePanel"></g:FlowPanel>


                <g:HTMLPanel ui:field = "profileUpLoadFormPanel"  >
                    <g:FormPanel  ui:field = "uploadForm" />
                </g:HTMLPanel>

    </g:HTMLPanel>
</ui:UiBinder>