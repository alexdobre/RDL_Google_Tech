<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:rdl='urn:import:com.therdl.client.view.widget'>
	<ui:style>



        .widgetlayout {
          margin:10px 0 0 30px;
          padding:10px 0 0 20px;

        }
        .searchlayout {
        position:absolute;
        margin:20px;
        padding:20px;
        width:600px;
        height:140px;
        }
	</ui:style>

	<g:HTMLPanel  >
        <rdl:AppMenu ui:field="appMenu"/>

        <g:DockLayoutPanel  ui:field="snipSearchDocPanel" unit='EM'>
            <g:north size='9'>
                <g:FlowPanel ui:field="snipSearchWidgetPanel" styleName='{style.searchlayout}'/>
            </g:north>
            <g:center>
                <g:FlowPanel  ui:field="snipListRow" styleName='{style.widgetlayout}' width="100%" height="100%" />
            </g:center>
        </g:DockLayoutPanel>

	</g:HTMLPanel>
</ui:UiBinder> 