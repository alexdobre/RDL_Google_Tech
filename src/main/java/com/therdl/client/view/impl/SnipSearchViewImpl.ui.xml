<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:rdl='urn:import:com.therdl.client.view.widget'>
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<rdl:AppMenu ui:field="appMenu" />
		<g:DockPanel>
			<g:Dock direction="WEST">
				<g:SimplePanel>
					<rdl:LeftMenuTree ui:field="leftMenuTree" />
				</g:SimplePanel>
			</g:Dock>
			<g:Dock direction="NORTH">
				<g:SimplePanel>
					<rdl:SnipSearchWidget ui:field="snipSearchWidget" width="100%" height="100%" />
				</g:SimplePanel>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:SimplePanel>
					<rdl:SnipListRowWidget ui:field="snipListRowWidgetTemp" width="100%" height="100%"/>
				</g:SimplePanel>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder> 