<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
		>
	<ui:with field="res" type="com.therdl.client.view.cssbundles.Resources"/>
	<ui:with field='i18n' type='com.therdl.client.view.i18n.I18NConstants'/>

	<b:Panel>
		<b:PanelHeader>
			<b:Heading size="H4">
				<b:Anchor dataToggle="COLLAPSE" dataParent="#accordion" ui:field="question"/>
			</b:Heading>
		</b:PanelHeader>
		<b:PanelCollapse in="false" ui:field="answer">
			<b:PanelBody ui:field="answerBody"/>
		</b:PanelCollapse>
	</b:Panel>
</ui:UiBinder>